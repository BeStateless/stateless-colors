{
  "name": "stateless-colors",
  "version": "0.1.1",
  "description": "a color management and manipulation module for stateless",
  "main": "dist/index.js",
  "scripts": {
    "test": "tsc -p . && mocha",
    "clean": "rm -rf dist",
    "build": "tsc webpack.config.ts && webpack",
    "build:watch": "tsc webpack.config.ts && webpack --watch --progress --colors",
    "posttest": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BeStateless/stateless-colors.git"
  },
  "keywords": [
    "colors",
    "stateless",
    "palette",
    "color manipulation"
  ],
  "author": "Emma Ramirez <ramirez.emma.g@gmail.com> emmaramirez.me",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BeStateless/stateless-colors/issues"
  },
  "homepage": "https://github.com/BeStateless/stateless-colors#readme",
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "awesome-typescript-loader": "^3.0.4-rc.2",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "mocha-typescript": "^1.0.21",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.4.2",
    "typescript": "^2.1.6",
    "webpack": "^2.2.1"
  }
